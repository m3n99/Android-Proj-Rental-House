<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <Button
            android:id="@+id/Tenant"
            android:layout_width="213dp"
            android:layout_height="68dp"
            android:backgroundTint="#009688"
            android:text="Tenant"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.303"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.45" />

        <Button
            android:id="@+id/Renting"
            android:layout_width="213dp"
            android:layout_height="68dp"
            android:backgroundTint="#AA2C05"
            android:text=" Renting Agency"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.919"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.616" />

        <ImageView
            android:id="@+id/imageViewsignT"
            android:layout_width="match_parent"
            android:layout_height="250dp"
            app:srcCompat="@drawable/tens"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_editor_absoluteY="29dp"></ImageView>

        <ImageView
            android:id="@+id/imageViewsignA"
            android:layout_width="match_parent"
            android:layout_height="250dp"
            android:layout_marginTop="184dp"
            app:layout_constraintTop_toBottomOf="@+id/imageViewsignT"
            app:srcCompat="@drawable/ags"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="0dp"></ImageView>

        <Button
            android:id="@+id/BackSA"
            android:layout_width="100dp"
            android:layout_height="68dp"
            android:backgroundTint="#000000"
            android:text=" Back"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.083"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.577" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</LinearLayout>