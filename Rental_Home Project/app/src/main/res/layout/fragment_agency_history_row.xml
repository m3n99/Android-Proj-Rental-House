<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/root"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/border_ui"
    android:clickable="true"
    android:tag="unique_tag">


    <TextView
        android:id="@+id/Hcity"
        android:layout_width="160dp"
        android:layout_height="25dp"
        android:layout_marginTop="12dp"
        android:text="City"
        app:layout_constraintEnd_toStartOf="@+id/HistoryCity"
        app:layout_constraintHorizontal_bias="0.313"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/HistoryCity"
        android:layout_width="160dp"
        android:layout_height="25dp"
        android:layout_marginTop="16dp"
        android:hint="City"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.824"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/Haddress"
        android:layout_width="160dp"
        android:layout_height="25dp"
        android:text="Postal Address"
        app:layout_constraintEnd_toStartOf="@+id/HistoryPAddress"
        app:layout_constraintHorizontal_bias="0.34"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Hcity" />

    <TextView
        android:id="@+id/HistoryPAddress"
        android:layout_width="160dp"
        android:layout_height="25dp"
        android:layout_marginTop="8dp"
        android:hint="PostalAddress"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.824"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/HistoryCity" />

    <TextView
        android:id="@+id/Hadate"
        android:layout_width="160dp"
        android:layout_height="25dp"
        android:layout_marginTop="40dp"
        android:text="Renting Period"
        app:layout_constraintEnd_toStartOf="@+id/HistoryPeriod"
        app:layout_constraintHorizontal_bias="0.34"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Hcity" />

    <TextView
        android:id="@+id/HistoryPeriod"
        android:layout_width="180dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:hint="Date"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.896"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/HistoryPAddress" />

    <TextView
        android:id="@+id/Hname"
        android:layout_width="160dp"
        android:layout_height="25dp"
        android:layout_marginTop="84dp"
        android:text="Tenant Name"
        app:layout_constraintEnd_toStartOf="@+id/HistoryFirstName"
        app:layout_constraintHorizontal_bias="0.319"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Hcity" />

    <TextView
        android:id="@+id/HistoryFirstName"
        android:layout_width="160dp"
        android:layout_height="25dp"
        android:hint="First Name"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.824"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/HistoryPeriod"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/HistoryLastName"
        android:layout_width="160dp"
        android:layout_height="25dp"
        android:hint="Last Name"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.808"
        app:layout_constraintStart_toEndOf="@+id/HistoryFirstName"
        app:layout_constraintTop_toBottomOf="@+id/HistoryPeriod"
        app:layout_constraintVertical_bias="1.0" />
</androidx.constraintlayout.widget.ConstraintLayout>